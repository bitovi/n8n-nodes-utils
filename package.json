{
  "name": "@bitovi/n8n-nodes-utils",
  "version": "0.2.4",
  "description": "N8N util nodes from Bitovi",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Roye",
    "email": "roye@bitovi.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitovi/n8n-nodes-utils.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes package.json",
    "copy": "rm -rf /Users/roye/.n8n/custom && mkdir /Users/roye/.n8n/custom && cp -R ./dist/nodes/. /Users/roye/.n8n/custom/"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/JSONSchemaValidation/Jsonschemavalidation.node.js",
      "dist/nodes/Regex/Regex.node.js",
      "dist/nodes/Skip/Skip.node.js",
      "dist/nodes/Wikipedia/Wikipedia.node.js"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.17.18",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "lodash": "^4.17.21",
    "wikipedia": "^2.1.2"
  }
}
